{
  "name": "@asxr/runtime-server",
  "version": "2.4.0",
  "description": "MICRO-ASXR Single-Page Runtime with REST API and NPX Server - Train with Colab, Deploy CPU/GPU",
  "main": "server.js",
  "bin": {
    "asxr": "./bin/cli.js",
    "asxr-server": "./bin/cli.js"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "train": "python python/asx_train.py",
    "build-model": "python python/asx_ngram_builder.py",
    "infer": "python python/asx_infer.py",
    "test": "jest --coverage",
    "colab": "echo 'Upload to Colab: https://colab.research.google.com/'"
  },
  "keywords": [
    "asxr",
    "runtime",
    "api",
    "machine-learning",
    "nlp",
    "language-model",
    "server",
    "npx",
    "colab",
    "gpu",
    "cpu"
  ],
  "author": "ASX-LABS",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.14.2",
    "node-fetch": "^2.7.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cannaseedus-bot/MICRO-ASXR.git"
  },
  "preferGlobal": true
}
